@model Vidly.Models.Movie
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.Genre.Name</li>
    <li>Release date: @Model.ReleasedDate </li>
    <li>Added date: @Model.DateAdded</li>
    <li>Stock: @Model.Stock</li>
</ul>

